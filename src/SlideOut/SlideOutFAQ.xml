<?xml version="1.0" encoding="utf-8" ?>
<widget id="SlideOut.widget.SlideOutFAQ" needsEntityContext="false" xmlns="http://www.mendix.com/widget/1.0/">
    <name>SlideOut FAQ</name>
    <description>Slide out widget to have the option of FAQ items about the current page.</description>
	<icon></icon>

    <properties>
        <property key="buttonString" type="translatableString" required="false">
            <caption>Button text</caption>
            <category>Appearance</category>
            <description>The text of the button.</description>
        </property>
		<property key="buttonClass" type="string" required="false">
            <caption>Button CSS Class</caption>
            <category>Appearance</category>
            <description>CSS class for the button</description>
        </property>
		<property key="headerButtonString" type="string" required="false">
			<caption>Header label text</caption>
			<category>Appearance</category>
			<description>Label for the header</description>
		</property>
		<property key="headerButtonClass" type="string" required="false">
			<caption>Header button CSS Class</caption>
			<category>Appearance</category>
			<description>CSS class for the header button</description>
		</property>
        <property key="picture" type="image" required="false">
			<caption>Picture</caption>
			<category>Appearance</category>
			<description>Select an image to add.</description>
		</property>
        <property key="contentWidth" type="integer" required="true" defaultValue="300">
            <caption>Width of the content</caption>
            <category>Appearance</category>
            <description>How large must the content be in pixels</description>
        </property>
		<property key="topPosition" type="integer" required="true" defaultValue="50">
            <caption>Top position</caption>
            <category>Appearance</category>
            <description>What is the minimal amout of pixels from the top?</description>
        </property>
		<property key="hideOthers" type="boolean" defaultValue="false">
			<caption>Hide other trays when this one opens</caption>
			<category>Appearance</category>
			<description>If true, other trays will hide when this one is activated. Otherwise, other trays can stay open.</description>
		</property>		
		<property key="seqPriority" type="integer" required="true" defaultValue="1">
            <caption>Priority</caption>
            <category>Appearance</category>
            <description>When there are multiple slideout widgets, the order of the buttons must be the same. The widgets will keep this order of the button according of the priority</description>
        </property>
        <property key="showTime" type="integer" required="true" defaultValue="500">
            <caption>Duration</caption>
            <category>Appearance</category>
            <description>The duration in milliseconds for the animation to happen.</description>
        </property>

		<property key="pageContent" type="form" required="true">
            <caption>Page</caption>
            <category>Data</category>
            <description>The content of that page will be loaded in the widget that must have a </description>
        </property>
		<property key="pageEntity" type="entity" allowNonPersistableEntities="true" required="true">
			<caption>Passthrough object</caption>
			<category>Data</category>
			<description>The object that will be pushed to the FAQ page to have the current page info. The widget will create the object when not instantiated.</description>
		</property>
		<property key="pageAttr" type="attribute" entityProperty="pageEntity" required="true">
			<caption>Current page</caption>
			<category>Data</category>
			<description>The attribute were the widget will place the current page to be pass throughed.</description>
			<attributeTypes>
				<attributeType name="String"/>
			</attributeTypes>
		</property>
        <property key="refreshMF"  type="microflow" required="false" entityProperty="pageEntity">
			<caption>Refresh content</caption>
			<category>Data</category>
			<description>When the property is filled, the microflow will be triggered when the slide is openend, but not the first time.</description>
			<returnType type="Void"/>
		</property>
		 
<property key="triggerClass" type="string" required="false">
	<caption>Extra Trigger Class</caption>
	<category>Behavior</category>
	<description>Any click on an element with this class will also toggle the slideout pane.</description>
</property>

    </properties>
</widget>
